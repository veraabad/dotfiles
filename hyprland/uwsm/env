# Changes require a relaunch of Hyprland to take effect.

# Ensure Omarchy bins are in the path
export DOTFILES_PATH=$HOME/.dotfiles
export PATH=$DOTFILES_PATH/bin/:$PATH
export TERMINAL=alacritty
export EDITOR="NVIM_APPNAME=nvb nvim"

# Set default terminal and editor
# source ~/.config/uwsm/default

echo $TERMINAL > ~/uwsm_out.txt
echo $EDITOR >> ~/uwsm_out.txt
echo $DOTFILES_PATH >> ~/uwsm_out.txt
